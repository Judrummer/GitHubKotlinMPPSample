CREATE TABLE viewer (
    id INTEGER PRIMARY KEY NOT NULL DEFAULT 0,
    github_id TEXT NOT NULL
);

upsertViewer:
INSERT OR REPLACE INTO viewer(id, github_id)
VALUES (0, ?);


selectAll:
SELECT *
FROM viewer;


selectViewer:
SELECT user.*
FROM user
INNER JOIN viewer
ON user.id = viewer.github_id
WHERE viewer.id = 0;
